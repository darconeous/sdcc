// peep 1
replace restart {
	movf	%1,w
	movwf	%2
	movf	%2,w
} by {
	; peep 1 - Removed redundant move
	movf	%1,w
	movwf	%2
}

// peep 2
replace restart {
	decf	%1,f
	movf	%1,w
	btfss	status,z
	goto	%2
} by {
	; peep 2 - decf/mov/skpz to decfsz
	decfsz	%1,f
	 goto	%2
}

